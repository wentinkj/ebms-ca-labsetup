export SUBJECT_ROOTCA="/C=$CERT_C/ST=$CERT_ST/L=$CERT_L/O=$CERT_O/OU=$CERT_OU/CN=ROOTCA"
export SUBJECT_INTERMEDIATECA="/C=$CERT_C/ST=$CERT_ST/L=$CERT_L/O=$CERT_O/OU=$CERT_OU/CN=INTERMEDIATECA1"